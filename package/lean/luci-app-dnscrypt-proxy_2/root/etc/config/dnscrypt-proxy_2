
config service 'dnscrypt'
	option enabled '0'
	option public_resolvers '1'
	list server_names 'rubyfish-ea'
	list server_names 'rubyfish-uw'
	list listen_addresses '127.0.0.1:5353'
	list listen_addresses '[::1]:5353'
	option max_clients '250'
	option ipv4_servers '1'
	option ipv6_servers '0'
	option dnscrypt_servers '1'
	option doh_servers '1'
	option require_dnssec '0'
	option require_nolog '0'
	option require_nofilter '1'
	option force_tcp '0'
	option timeout '2500'
	option keepalive '30'
	option cert_refresh_delay '240'
	option fallback_resolver '114.114.114.114:53'
	option ignore_system_dns '0'
	option netprobe_timeout '60'
	option block_ipv6 '1'
	option cache '1'

